{
  "name": "u8-mqtt",
  "description": "MQTT client using Uint8Array -- suitable for use in the Browser, NodeJS, and Deno.land.",
  "version": "0.0.17",
  "license": "BSD-2-Clause",
  "author": "Shane Holloway <shane.holloway@ieee.org>",
  "homepage": "https://github.com/shanewholloway/js-u8-mqtt#readme",
  "bugs": {
    "url": "https://github.com/shanewholloway/js-u8-mqtt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/shanewholloway/js-u8-mqtt.git"
  },
  "files": [
    "cjs/",
    "code/",
    "esm/"
  ],
  "type": "module",
  "main": "./cjs/index.cjs",
  "module": "./esm/index.mjs",
  "exports": {
    "./": "./",
    ".": {
      "import": "./esm/index.mjs",
      "require": "./cjs/index.cjs"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "qsrv": "^0.1.7",
    "regexparam": "^1.3.0",
    "rollup": "^2.28.2",
    "rollup-plugin-dgnotify": "^0.1.7",
    "rollup-plugin-jsy": "^1.7.1",
    "rollup-plugin-terser": "^7.0.2",
    "u8-mqtt-packet": "^0.1.2"
  },
  "scripts": {
    "start": "qsrv -- *.html esm/",
    "clean": "rm -rf ./cjs/* ./esm/*",
    "build": "rollup --config",
    "watch": "npm -s run build -- --watch",
    "demo:node": "node ./demo/node_v4.mjs",
    "demo:deno": "deno run --allow-net ./demo/deno_v4.mjs",
    "pretest": "npm -s run build",
    "test": "true"
  }
}
